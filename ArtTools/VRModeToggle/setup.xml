  <Public version="1.0" debug="true" showFPS="true" useVRInput="True" windDir="Left" />
  <Network minPlayer="1" maxPlayer="8" isOffline="true">
    <NetworkSetting1 isHost="true" ip="172.16.0.80" port="14568" initWeaponType="RIFLE" initPlayerStyle="Default" />
    <NetworkSetting2 isHost="false" ip="172.16.1.237" port="14568" initWeaponType="RIFLE" initPlayerStyle="Default" />
