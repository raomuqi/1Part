    <!-- 
		硬件代理设置，负责处理远程硬件调用(音频，震动，风扇...) 
		ip = 硬件服务器ip，serversPort = 硬件服务器端口，clientPort = 硬件客户端端口(主机与所有客户端)
		-->
    <HardwareProxySetting ip="172.16.0.80" serversPort="14569" clientPort="14570" />
  </Network>
  <!-- 
		x,y,z是属于平台偏移,y也用于人物站立点偏移(一般用于自动校准后的偏移)
	-->
